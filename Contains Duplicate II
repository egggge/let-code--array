#自己的思路，依旧嵌套循环。当然又是超时
class Solution {
public:
    bool containsNearbyDuplicate(vector<int>& nums, int k) {


        for(int i=0;i<nums.size();i++){
            for(int j=i+1;j<nums.size();j++){
                if(nums[i]==nums[j]&&(j-i)<=k){
                    return 1;
                }
            }
        }
        
       return 0; 
    }
};
